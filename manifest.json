{
  "name": "Visual History",
  "description": "Tree-based browser history navigation and visualization",
  "version": "1.1.4",
  "author": "Joel Gustafson and Kenneth Friedman",
  "permissions": [
    "tabs",
    "webNavigation",
    "activeTab",
    "<all_urls>",
    "contextMenus"
  ],
  "background": {
    "persistent": true,
    "scripts": ["color-thief.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["D3/d3.min.js", "content.js"]
    }
  ],
  "icons": {
    "128": "icon.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon38.png"
  },
  "web_accessible_resources": ["tab.png", "tree.css"],
  "commands": {
    "move-up": {
      "suggested_key": {
        "default": "Ctrl+Up"
      },
      "description": "Navigate up the tree"
    },
    "move-down": {
      "suggested_key": {
        "default": "Ctrl+Down"
      },
      "description": "Navigate down the tree"
    },
    "move-left": {
      "suggested_key": {
        "default": "Ctrl+Left"
      },
      "description": "Navigate to the left in the tree"
    },
    "move-right": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "Navigate to the right in the tree"
    }
  },
  "manifest_version": 2
}
